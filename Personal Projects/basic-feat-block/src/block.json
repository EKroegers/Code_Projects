{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/basic-feat-block",
	"version": "1.2.0",
	"title": "Basic Feat Block",
	"category": "widgets",
	"description": "A Block for TTRPG Feat content incuding Title, Flavor, Prerequisites, Benefits, and Special fields",
	"example": {},
  "attributes": {
    "featTitle": {
      "type": "string",
      "default": ""
    },
    "showFlavorField": {
      "type": "boolean",
      "default": false
    },
    "featFlavor": {
      "type": "string",
      "default": ""
    },
    "featPrerequisites": {
      "type": "string",
      "default": ""
    },
    "featBenefits": {
      "type": "string",
      "default": ""
    },
    "showSpecialField": {
      "type": "boolean",
      "default": false
    },
    "featSpecials": {
      "type": "string",
      "default": ""
    }
  },
	"supports": {
    "color": {
      "background": false,
      "text": true
    },
    "html": false,
    "typography": {
      "fontSize": true
    }
	},
	"textdomain": "basic-feat-block",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
